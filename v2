local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Window
local Window = Rayfield:CreateWindow({
    Name = "üåÄ ‡πÑ‡∏•‡∏ü‡πå‡∏™‡∏≤‡∏£‡∏∞ ‡πÑ‡∏õ‡∏ß‡∏±‡∏ô‡πÜ v1.5 (God Mode)",
    LoadingTitle = "‡∏¢‡∏¥‡∏ô‡∏î‡∏µ‡∏ï‡πâ‡∏≠‡∏ô‡∏£‡∏±‡∏ö Power Panel v1 ‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏Ç‡∏≠‡∏á‡πÑ‡∏•‡∏ü‡πå‡∏™‡∏≤‡∏£‡∏∞‡πÑ‡∏õ‡∏ß‡∏±‡∏ô‡πÜ",
    LoadingSubtitle = "‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÇ‡∏´‡∏•‡∏î...",
    ConfigurationSaving = {
        Enabled = true,
        FolderName = "LifeSuraConfig",
        FileName = "SuraConfig"
    },
    Discord = {
        Enabled = false,
        Invite = "dSyCsRpU62",
        RememberJoins = true
    },
    KeySystem = false
})

-- ================== [ ‡∏õ‡∏∏‡πà‡∏°‡∏•‡∏≠‡∏¢ Toggle ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏°‡∏∑‡∏≠‡∏ñ‡∏∑‡∏≠ ] ==================
do
    local Players = game:GetService("Players")
    local CoreGui = game:GetService("CoreGui")
    local UIS = game:GetService("UserInputService")

    local function safeParent()
        local ok, ui = pcall(function()
            if gethui then return gethui() end
            return CoreGui
        end)
        return ok and ui or CoreGui
    end

    local function getRayfieldGui()
        return CoreGui:FindFirstChild("Rayfield")
            or (Players.LocalPlayer.PlayerGui and Players.LocalPlayer.PlayerGui:FindFirstChild("Rayfield"))
    end

    local toggleGui = Instance.new("ScreenGui")
    toggleGui.Name = "LifeSura_Toggle"
    toggleGui.IgnoreGuiInset = true
    toggleGui.ResetOnSpawn = false
    toggleGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    toggleGui.Parent = safeParent()

    local btn = Instance.new("ImageButton")
    btn.Name = "DockButton"
    btn.Size = UDim2.fromOffset(56, 56)
    btn.BackgroundTransparency = 0.15
    btn.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
    btn.Image = "rbxassetid://6031280882" -- ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡πÑ‡∏î‡πâ‡∏ï‡∏≤‡∏°‡πÉ‡∏à
    btn.AutoButtonColor = true
    btn.ZIndex = 10
    btn.Parent = toggleGui
    btn.Position = getgenv().LifeSuraTogglePos or UDim2.new(1, -72, 1, -96)

    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 999)
    corner.Parent = btn
    local stroke = Instance.new("UIStroke")
    stroke.Thickness = 1
    stroke.Color = Color3.fromRGB(255, 255, 255)
    stroke.Transparency = 0.3
    stroke.Parent = btn

    local uiVisible = true
    local function setVisible(on)
        local rf = getRayfieldGui()
        if rf then
            rf.Enabled = on
            uiVisible = on
        end
    end

    task.spawn(function()
        local t0 = time()
        while not getRayfieldGui() and time() - t0 < 5 do task.wait() end
        setVisible(true)
    end)

    local dragging = false
    local dragStart, startPos
    local clickBeganAt = 0
    local movedEnough = false

    local function clampToScreen(pos)
        local cam = workspace.CurrentCamera
        if not cam then return pos end
        local vs = cam.ViewportSize
        local x = math.clamp(pos.X.Offset, 8, vs.X - btn.AbsoluteSize.X - 8)
        local y = math.clamp(pos.Y.Offset, 8, vs.Y - btn.AbsoluteSize.Y - 8)
        return UDim2.fromOffset(x, y)
    end

    btn.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1
        or input.UserInputType == Enum.UserInputType.Touch then
            dragging = true
            movedEnough = false
            clickBeganAt = tick()
            dragStart = input.Position
            startPos = btn.Position
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragging = false
                    if not movedEnough and (tick() - clickBeganAt) < 0.25 then
                        setVisible(not uiVisible)
                    end
                    getgenv().LifeSuraTogglePos = btn.Position
                end
            end)
        end
    end)

    UIS.InputChanged:Connect(function(input)
        if dragging and (input.UserInputType == Enum.UserInputType.MouseMovement
            or input.UserInputType == Enum.UserInputType.Touch) then

            local delta = input.Position - dragStart
            if delta.Magnitude > 6 then movedEnough = true end

            local newPos = UDim2.new(
                startPos.X.Scale, startPos.X.Offset + delta.X,
                startPos.Y.Scale, startPos.Y.Offset + delta.Y
            )
            btn.Position = clampToScreen(newPos)
        end
    end)

    local lastTap = 0
    btn.MouseButton1Click:Connect(function()
        local now = tick()
        if now - lastTap < 0.25 then
            setVisible(true)
        end
        lastTap = now
    end)

    workspace:GetPropertyChangedSignal("CurrentCamera"):Connect(function()
        task.defer(function()
            if workspace.CurrentCamera then
                btn.Position = clampToScreen(btn.Position)
            end
        end)
    end)

    UIS.InputBegan:Connect(function(input, gpe)
        if gpe then return end
        if input.KeyCode == Enum.KeyCode.F8 then
            setVisible(not uiVisible)
        end
    end)
end
-- ================== [ ‡∏à‡∏ö‡∏õ‡∏∏‡πà‡∏°‡∏•‡∏≠‡∏¢ ] ==================

-- ‡πÅ‡∏ó‡πá‡∏ö‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£
local MainTab = Window:CreateTab("‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£", 6026568198)

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏£‡∏∞‡∏ö‡∏ö
local Players = game:GetService("Players")
local RS = game:GetService("RunService")
local UIS = game:GetService("UserInputService")
local player = Players.LocalPlayer
local char = player.Character or player.CharacterAdded:Wait()
local hrp = char:WaitForChild("HumanoidRootPart")
local humanoid = char:WaitForChild("Humanoid")

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô
local flying = false
local vv, gyro
local flySpeed = 50
local superSpeed = false
local runSpeed = 32
local noclipEnabled = false
local infJumpEnabled = false

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ö‡∏¥‡∏ô
local function startFlying()
    if flying then return end
    flying = true
    humanoid.PlatformStand = true

    vv = Instance.new("BodyVelocity")
    vv.MaxForce = Vector3.new(1e5,1e5,1e5)
    vv.P = 1250
    vv.Velocity = Vector3.zero
    vv.Parent = hrp

    gyro = Instance.new("BodyGyro")
    gyro.MaxTorque = Vector3.new(4e5,4e5,4e5)
    gyro.P = 3000
    gyro.CFrame = hrp.CFrame
    gyro.Parent = hrp

    Rayfield:Notify({
        Title = "‚úàÔ∏è ‡πÇ‡∏´‡∏°‡∏î‡∏ö‡∏¥‡∏ô‡πÄ‡∏õ‡∏¥‡∏î!",
        Content = "‡πÉ‡∏ä‡πâ W,A,S,D,Space,Ctrl ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏Ñ‡∏ß‡∏ö‡∏Ñ‡∏∏‡∏°",
        Duration = 3,
        Image = 6026568198
    })
end

local function stopFlying()
    if not flying then return end
    flying = false
    if vv then vv:Destroy() vv=nil end
    if gyro then gyro:Destroy() gyro=nil end
    humanoid.PlatformStand = false

    Rayfield:Notify({
        Title = "üõ¨ ‡πÇ‡∏´‡∏°‡∏î‡∏ö‡∏¥‡∏ô‡∏õ‡∏¥‡∏î",
        Content = "‡∏•‡∏á‡∏à‡∏≠‡∏î‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢",
        Duration = 3,
        Image = 6026568198
    })
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ß‡∏¥‡πà‡∏á‡πÄ‡∏£‡πá‡∏ß
local function toggleSpeed(on)
    superSpeed = on
    humanoid.WalkSpeed = superSpeed and runSpeed or 16
    Rayfield:Notify({
        Title = superSpeed and "üèÉ‚Äç‚ôÇÔ∏è ‡∏ß‡∏¥‡πà‡∏á‡πÄ‡∏£‡πá‡∏ß‡πÄ‡∏õ‡∏¥‡∏î" or "üö∂‚Äç‚ôÇÔ∏è ‡∏ß‡∏¥‡πà‡∏á‡πÄ‡∏£‡πá‡∏ß‡∏õ‡∏¥‡∏î",
        Content = superSpeed and "‡∏ß‡∏¥‡πà‡∏á‡πÄ‡∏£‡πá‡∏ß‡∏™‡∏∏‡∏î‡∏Ç‡∏µ‡∏î!" or "‡∏Å‡∏•‡∏±‡∏ö‡∏™‡∏π‡πà‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏õ‡∏Å‡∏ï‡∏¥",
        Duration = 3,
        Image = 6023426921
    })
end

-- Toggle & Slider UI
local flyToggle = MainTab:CreateToggle({
    Name="‚úàÔ∏è ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î ‡∏Å‡∏≤‡∏£‡∏ö‡∏¥‡∏ô",
    CurrentValue=false,
    Flag="FlyToggle",
    Callback=function(val)
        if val then startFlying() else stopFlying() end
    end
})

local flySlider = MainTab:CreateSlider({
    Name="üöÄ ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ö‡∏¥‡∏ô",
    Range={1,1000},
    Increment=1,
    Suffix=" Speed",
    CurrentValue=50,
    Flag="FlySpeed",
    Callback=function(val)
        flySpeed=val
        Rayfield:Notify({
            Title="üöÄ ‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ö‡∏¥‡∏ô",
            Content="‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ö‡∏¥‡∏ô‡πÄ‡∏õ‡πá‡∏ô "..val,
            Duration=2,
            Image = 6026568198
        })
    end
})

local speedToggle = MainTab:CreateToggle({
    Name="üèÉ‚Äç‚ôÇÔ∏è ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î ‡∏ß‡∏¥‡πà‡∏á‡πÄ‡∏£‡πá‡∏ß",
    CurrentValue=false,
    Flag="SpeedToggle",
    Callback=toggleSpeed
})

local speedSlider = MainTab:CreateSlider({
    Name="‚ö° ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ß‡∏¥‡πà‡∏á",
    Range={1,1000},
    Increment=1,
    Suffix=" Speed",
    CurrentValue=32,
    Flag="RunSpeed",
    Callback=function(val)
        runSpeed=val
        if superSpeed then humanoid.WalkSpeed=runSpeed end
        Rayfield:Notify({
            Title="‚ö° ‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ß‡∏¥‡πà‡∏á",
            Content="‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ß‡∏¥‡πà‡∏á‡πÄ‡∏õ‡πá‡∏ô "..val,
            Duration=2,
            Image=6023426921
        })
    end
})

local noclipToggle = MainTab:CreateToggle({
    Name="üëª ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î Noclip",
    CurrentValue=false,
    Flag="NoclipToggle",
    Callback=function(val)
        noclipEnabled=val
        Rayfield:Notify({
            Title=val and "üëª Noclip ‡πÄ‡∏õ‡∏¥‡∏î" or "üõë Noclip ‡∏õ‡∏¥‡∏î",
            Content=val and "‡πÄ‡∏î‡∏¥‡∏ô‡∏ó‡∏∞‡∏•‡∏∏‡∏ß‡∏±‡∏ï‡∏ñ‡∏∏‡πÑ‡∏î‡πâ!" or "‡∏Å‡∏•‡∏±‡∏ö‡∏õ‡∏Å‡∏ï‡∏¥",
            Duration=3,
            Image=6026568198
        })
    end
})

local infJumpToggle = MainTab:CreateToggle({
    Name="ü¶ò ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î Infinite Jump",
    CurrentValue=false,
    Flag="InfJumpToggle",
    Callback=function(val)
        infJumpEnabled=val
        Rayfield:Notify({
            Title=val and "ü¶ò Infinite Jump ‡πÄ‡∏õ‡∏¥‡∏î" or "üõë Infinite Jump ‡∏õ‡∏¥‡∏î",
            Content=val and "‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡πÑ‡∏°‡πà‡∏à‡∏≥‡∏Å‡∏±‡∏î!" or "‡∏Å‡∏•‡∏±‡∏ö‡∏™‡∏π‡πà‡∏õ‡∏Å‡∏ï‡∏¥",
            Duration=3,
            Image=6026568198
        })
    end
})

-- ‡πÅ‡∏ó‡πá‡∏ö‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ô
local friendTab = Window:CreateTab("üë• ‡∏ß‡∏≤‡∏õ", 6026568198)

local function getPlayerList()
    local list = {}
    for _, p in pairs(Players:GetPlayers()) do
        if p ~= player then
            table.insert(list, p.DisplayName.." ("..p.Name..")")
        end
    end
    return list
end

local friendDropdown = friendTab:CreateDropdown({
    Name = "üë• ‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ô",
    Options = getPlayerList(),
    CurrentOption = {},
    Flag = "FriendSelect"
})

friendTab:CreateButton({
    Name = "üìå ‡∏ß‡∏≤‡∏õ‡πÑ‡∏õ‡∏´‡∏≤‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ô",
    Callback = function()
        local selected = friendDropdown.CurrentOption[1]
        if not selected or selected == "" then
            Rayfield:Notify({
                Title = "‚ùå Error",
                Content = "‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ô‡∏Å‡πà‡∏≠‡∏ô!",
                Duration = 3,
                Image = 6026568198
            })
            return
        end

        local friendName = selected:match("%((.-)%)") or selected

        local char = player.Character or player.CharacterAdded:Wait()
        local hrp = char:WaitForChild("HumanoidRootPart")
        local humanoid = char:WaitForChild("Humanoid")

        local targetCFrame = nil
        local targetPlayer = Players:FindFirstChild(friendName)
        if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
            targetCFrame = targetPlayer.Character.HumanoidRootPart.CFrame
        else
            for _, obj in pairs(workspace:GetChildren()) do
                if obj:IsA("Model") and obj.Name:lower() == friendName:lower() then
                    local part = obj:FindFirstChildWhichIsA("BasePart")
                    if part then
                        targetCFrame = part.CFrame
                        break
                    end
                end
            end
        end

        if targetCFrame then
            hrp.CFrame = targetCFrame + Vector3.new(0,3,0)
            Rayfield:Notify({
                Title = "‚úÖ Teleport ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à",
                Content = "‡∏Ñ‡∏∏‡∏ì‡πÑ‡∏î‡πâ‡πÑ‡∏õ‡∏´‡∏≤ "..friendName,
                Duration = 3,
                Image = 6026568198
            })
        else
            Rayfield:Notify({
                Title = "‚ùå ‡πÑ‡∏°‡πà‡∏û‡∏ö‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢",
                Content = "‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ô‡πÑ‡∏°‡πà‡∏≠‡∏≠‡∏ô‡πÑ‡∏•‡∏ô‡πå ‡∏´‡∏£‡∏∑‡∏≠‡πÇ‡∏°‡πÄ‡∏î‡∏•‡πÑ‡∏°‡πà‡∏≠‡∏¢‡∏π‡πà",
                Duration = 3,
                Image = 6026568198
            })
        end
    end
})

friendTab:CreateButton({
    Name = "üîÑ ‡∏£‡∏µ‡πÄ‡∏ü‡∏£‡∏ä‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠",
    Callback = function()
        friendDropdown:Set(getPlayerList())
        Rayfield:Notify({
            Title = "üîÑ ‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡πÅ‡∏•‡πâ‡∏ß",
            Content = "‡πÇ‡∏´‡∏•‡∏î‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÉ‡∏´‡∏°‡πà‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à!",
            Duration = 3,
            Image = 6026568198
        })
    end
})

