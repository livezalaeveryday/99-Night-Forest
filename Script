    end,
})
 
local MainTab = Window:CreateTab("‡πÄ‡∏ó‡∏™",6035056489)
 
local MainTab = Window:CreateTab("üìú ‡πÄ‡∏Ñ‡∏£‡∏î‡∏¥‡∏ï", 6034989579)
 
