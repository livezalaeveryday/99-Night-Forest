MainTab:CreateButton({Name = "‡πÄ‡∏ï‡∏¥‡∏°‡πÑ‡∏ü", Callback = function() loadstring(game:HttpGet("https://raw.githubusercontent.com/livezalaeveryday/99-Night-Forest/refs/heads/main/getfire"))() end})
MainTab:CreateButton({Name = "‡πÄ‡∏ï‡∏¥‡∏°‡∏≠‡∏≤‡∏´‡∏≤‡∏£", Callback = function() loadstring(game:HttpGet("https://raw.githubusercontent.com/livezalaeveryday/99-Night-Forest/refs/heads/main/food"))() end})
MainTab:CreateButton({Name = "‡πÄ‡∏ï‡∏¥‡∏°‡∏¢‡∏≤", Callback = function() loadstring(game:HttpGet("https://raw.githubusercontent.com/livezalaeveryday/99-Night-Forest/refs/heads/main/getbandage"))() end})
MainTab:CreateToggle({
    Name = "‡πÄ‡∏ï‡∏¥‡∏°‡πÄ‡∏Ñ‡∏£‡∏∑‡πà‡∏≠‡∏á‡∏ö‡∏î (Stack)",
    CurrentValue = false,
    Flag = "IronStackToggle",
