   Name = "üåÄ Power Panel v3.0",
   LoadingTitle = "‡∏¢‡∏¥‡∏ô‡∏î‡∏µ‡∏ï‡πâ‡∏≠‡∏ô‡∏£‡∏±‡∏ö‡∏™‡∏π‡πà‡πÑ‡∏•‡∏ü‡πå‡∏™‡∏≤‡∏£‡∏∞ ‡πÑ‡∏õ‡∏ß‡∏±‡∏ô‡πÜ",
   LoadingSubtitle = "‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÇ‡∏´‡∏•‡∏î...",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = "LifeSuraConfig",
      FileName = "SuraConfig"
   },
   Discord = {
      Enabled = false,
      Invite = "dSyCsRpU62",
      RememberJoins = true
   },
   KeySystem = false
