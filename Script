-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Tab ‡∏û‡∏£‡πâ‡∏≠‡∏°‡πÑ‡∏≠‡∏Ñ‡∏≠‡∏ô‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£
local MainTab = Window:CreateTab("‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£", 6026568198)
 
-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏£‡∏∞‡∏ö‡∏ö
local Players = game:GetService("Players")
local RS = game:GetService("RunService")
local UIS = game:GetService("UserInputService")
 
local player = Players.LocalPlayer
local char = player.Character or player.CharacterAdded:Wait()
local hrp = char:WaitForChild("HumanoidRootPart")
local humanoid = char:WaitForChild("Humanoid")
 
-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ö‡∏¥‡∏ô
local flying = false
local vv, gyro
local flySpeed = 50
 
-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ß‡∏¥‡πà‡∏á
local superSpeed = false
local runSpeed = 32
 
-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô Noclip
local noclipEnabled = false
 
 
local function startFlying()
    if flying then return end
    if not hrp or not humanoid then return end
    flying = true
    humanoid.PlatformStand = true
 
    vv = Instance.new("BodyVelocity")
    vv.MaxForce = Vector3.new(1e5, 1e5, 1e5)
    vv.P = 1250
    vv.Velocity = Vector3.zero
    vv.Parent = hrp
 
    gyro = Instance.new("BodyGyro")
    gyro.MaxTorque = Vector3.new(4e5, 4e5, 4e5)
    gyro.P = 3000
    gyro.CFrame = hrp.CFrame
    gyro.Parent = hrp
 
    Rayfield:Notify({
       Title = "‚úàÔ∏è ‡πÇ‡∏´‡∏°‡∏î‡∏ö‡∏¥‡∏ô‡πÄ‡∏õ‡∏¥‡∏î!",
       Content = "‡∏ö‡∏¥‡∏ô‡πÑ‡∏î‡πâ‡πÅ‡∏•‡πâ‡∏ß! ‡πÉ‡∏ä‡πâ W, A, S, D, Space, ‡πÅ‡∏•‡∏∞ Ctrl ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏Ñ‡∏ß‡∏ö‡∏Ñ‡∏∏‡∏°",
       Duration = 3,
       Image = 6026568198,
    })
end
 
local function stopFlying()
    if not flying then return end
    flying = false
    if vv then vv:Destroy() vv = nil end
    if gyro then gyro:Destroy() gyro = nil end
    humanoid.PlatformStand = false
 
    Rayfield:Notify({
       Title = "üõ¨ ‡πÇ‡∏´‡∏°‡∏î‡∏ö‡∏¥‡∏ô‡∏õ‡∏¥‡∏î",
       Content = "‡∏•‡∏á‡∏à‡∏≠‡∏î‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢‡πÅ‡∏•‡πâ‡∏ß!",
       Duration = 3,
       Image = 6026568198,
    })
end
 
 
local function toggleSpeed(on)
    superSpeed = on
    if superSpeed then
        humanoid.WalkSpeed = runSpeed
        Rayfield:Notify({
           Title = "üèÉ‚Äç‚ôÇÔ∏è ‡πÇ‡∏´‡∏°‡∏î‡∏ß‡∏¥‡πà‡∏á‡πÄ‡∏£‡πá‡∏ß‡πÄ‡∏õ‡∏¥‡∏î!",
           Content = "‡∏ß‡∏¥‡πà‡∏á‡πÄ‡∏£‡πá‡∏ß‡∏™‡∏∏‡∏î‡∏Ç‡∏µ‡∏î! ‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡πÑ‡∏î‡πâ‡∏ó‡∏µ‡πà‡∏™‡πÑ‡∏•‡πÄ‡∏î‡∏≠‡∏£‡πå",
           Duration = 3,
           Image = 6023426921, -- ‡πÑ‡∏≠‡∏Ñ‡∏≠‡∏ô‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß
        })
    else
        humanoid.WalkSpeed = 16
        Rayfield:Notify({
           Title = "üö∂‚Äç‚ôÇÔ∏è ‡πÇ‡∏´‡∏°‡∏î‡∏ß‡∏¥‡πà‡∏á‡πÄ‡∏£‡πá‡∏ß‡∏õ‡∏¥‡∏î",
           Content = "‡∏Å‡∏•‡∏±‡∏ö‡∏™‡∏π‡πà‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏õ‡∏Å‡∏ï‡∏¥",
           Duration = 3,
           Image = 6023426921,
        })
    end
end
 
 
local flyToggle = MainTab:CreateToggle({
   Name = "‚úàÔ∏è ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î ‡∏Å‡∏≤‡∏£‡∏ö‡∏¥‡∏ô",
   CurrentValue = false,
   Flag = "FlyToggle",
   Callback = function(Value)
       if Value then
           startFlying()
       else
           stopFlying()
       end
   end,
})
 
local flySlider = MainTab:CreateSlider({
   Name = "üöÄ ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ö‡∏¥‡∏ô",
   Range = {1, 500},
   Increment = 1,
   Suffix = " Speed",
   CurrentValue = 50,
   Flag = "FlySpeed",
   Callback = function(Value)
       flySpeed = Value
       Rayfield:Notify({
          Title = "üöÄ ‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ö‡∏¥‡∏ô",
          Content = "‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ö‡∏¥‡∏ô‡∏ï‡∏±‡πâ‡∏á‡πÄ‡∏õ‡πá‡∏ô " .. Value .. " ‡πÅ‡∏•‡πâ‡∏ß!",
          Duration = 2,
          Image = 6026568198,
       })
   end,
})
 
 
local speedToggle = MainTab:CreateToggle({
   Name = "üèÉ‚Äç‚ôÇÔ∏è ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î ‡∏ß‡∏¥‡πà‡∏á‡πÄ‡∏£‡πá‡∏ß",
   CurrentValue = false,
   Flag = "SpeedToggle",
   Callback = function(Value)
       toggleSpeed(Value)
   end,
})
 
local speedSlider = MainTab:CreateSlider({
   Name = "‚ö° ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ß‡∏¥‡πà‡∏á",
   Range = {1, 500},
   Increment = 1,
   Suffix = " Speed",
   CurrentValue = 32,
   Flag = "RunSpeed",
   Callback = function(Value)
       runSpeed = Value
       if superSpeed then
           humanoid.WalkSpeed = runSpeed
       end
       Rayfield:Notify({
          Title = "‚ö° ‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ß‡∏¥‡πà‡∏á",
          Content = "‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ß‡∏¥‡πà‡∏á‡∏ï‡∏±‡πâ‡∏á‡πÄ‡∏õ‡πá‡∏ô " .. Value .. " ‡πÅ‡∏•‡πâ‡∏ß!",
          Duration = 2,
          Image = 6023426921,
       })
   end,
})
 
-- Noclip
local noclipToggle = MainTab:CreateToggle({
